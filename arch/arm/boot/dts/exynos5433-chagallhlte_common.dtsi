#include "exynos5433.dtsi"

/ {

	model = "Samsung UNIVERSAL5433 board based on EXYNOS5433";
	compatible = "samsung,UNIVERSAL5433", "samsung,exynos5433";

	aliases {
		spi0 = &spi_0;
		spi1 = &spi_1;
		spi2 = &spi_2;
		spi3 = &spi_3;
		spi4 = &spi_4;
		spi5 = &spi_5;
		spi6 = &spi_6;
	};

	chosen {
		bootargs = "console=ttySAC1,115200 vmalloc=256M";
	};

	fixed-rate-clocks {
		oscclk {
			compatible = "samsung,exynos5433-oscclk";
			clock-frequency = <24000000>;
		};
	};

	dwmmc0@15540000 {
		status = "okay";
		num-slots = <1>;
		broken-cd;
		bypass-smu;
		clock-gate;
		enable-cclk-on-suspend;
		fixed_volt;
		only_once_tune;
		supports-highspeed;
		supports-hs200-1-8v-mode;
		supports-ddr200-1-8v-mode;
		supports-ddr200-enhanced-strobe;
		supports-poweroff-notification;
		device-driver = <1>;
		enable-cache-control;
		enable-no-sleep-cmd;
		use-fine-tuning;
		use-enable-shift;
		ignore-phase = <(1 << 7 | 1 << 6)>;
		extra_tuning;
		bypass-for-allpass;
		desc-size = <4>;
		fifo-depth = <0x80>;
		qos_int_level = <400000>;
		use-enable-shift;
		card-detect-delay = <200>;
		samsung,dw-mshc-ddr200-delay-line = <0x40>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 0>;
		samsung,dw-mshc-sdr-hs-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		samsung,dw-mshc-hs200-timing = <3 0 3 0>;
		samsung,dw-mshc-ddr200-timing = <1 0 3 0>;
		clk_pin = "gpr0-0";
		clk_addr = "15690000.pinctrl";
		clk_val = <0x3>;
		clk_str_num = <6>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_rdqs &sd0_bus1 &sd0_bus4 &sd0_bus8>;
		gpios = <&gpd6 6 0>;
		slot@0 {
			reg = <0>;
			bus-width = <8>;
		};
	};

	dwmmc1@15550000 {
		status = "disabled";
		num-slots = <1>;
		fixed_volt;
		clock-gate;
		enable-cclk-on-suspend;
		only_once_tune;
		supports-highspeed;
		not-allow-single-dma;
		desc-size = <4>;
		qos_int_level = <133>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <200000>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus1 &sd1_bus4>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
		};
	};

	dwmmc2@15560000 {
		status = "okay";
		num-slots = <1>;
		bypass-smu;
		caps-control;
		supports-cmd23;
		supports-erase;
		supports-highspeed;
		supports-sdr50-mode;
		clock-gate;
		enable-cclk-on-suspend;
		fix-fmp-size-mismatch;
		not-allow-single-dma;
		desc-size = <4>;
		use-fine-tuning;
		bypass-for-allpass;
		cd-type = <0>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <400000>;
		ignore-phase = <(1 << 7)>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		clk_pin = "gpr4-0";
		clk_addr = "15690000.pinctrl";
		clk_val = <0x2>;
		clk_str_num = <6>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default", "output";
		pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_bus1 &sd2_bus4 &dwmmc2_cd_ext_irq &dwmmc2_vmmc_en_gpio>;
		pinctrl-1 = <&sd2_clk_output &sd2_cmd_output &sd2_bus1 &sd2_bus4 &dwmmc2_cd_ext_irq &dwmmc2_vmmc_en_gpio>;

		gpio-controller;
		#gpio-cells = <2>;
		cd-gpio = <&gpa2 4 0xf>;

		vmmc-en-gpio = <&gpf0 7 0x1>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};
	};

	pinctrl@11090000 {
		dwmmc2_vmmc_en_gpio: dwmmc2-vmmc-en-gpio {
			samsung,pins = "gpf0-7";
			samsung,pin-function = <0x1>;
			samsung,pin-pud = <0>;
		};
	};

	serial@11460000 {
		samsung,uart-wake-peer;
		cpu_qos_val = <800000>;
		qos_timeout = <1000000>;
	};

	pinctrl@10580000 {
		ok_sw: ok-sw {
			samsung,pins = "gpa0-3";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		dwmmc2_cd_ext_irq: dwmmc2_cd_ext_irq {
			samsung,pins = "gpa2-4";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		pmic_irq: pmic-irq {
			samsung,pins = "gpa0-7";
			samsung,pin-pud = <0>;
			samsung,pin-drv = <3>;
		};
		/* Warm reset information from AP */
		pm_wrsti: pm-wrsti {
			samsung,pins = "gpf5-4";
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <3>;
		};

		gpa3_pull_up: gpa3_pull_up_for_smpl {
			samsung,pins = "gpa3-7";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&ok_sw>;
		button@1 {
			label = "gpio-keys: KEY_POWER";
			interrupts = <7 0 0>;
			interrupt-parent = <&gpa2>;
			linux,code = <116>;
			gpios = <&gpa2 7 0xf>;
			gpio-key,wakeup = <1>;
		};
		button@2 {
			label = "gpio-keys: KEY_VOLUMEDOWN";
			interrupts = <1 0 0>;
			interrupt-parent = <&gpa2>;
			linux,code = <114>;
			gpios = <&gpa2 1 0xf>;
		};
		button@3 {
			label = "gpio-keys: KEY_VOLUMEUP";
			interrupts = <0 0 0>;
			interrupt-parent = <&gpa2>;
			linux,code = <115>;
			gpios = <&gpa2 0 0xf>;
		};
		button@4 {
			label = "gpio-keys: KEY_HOMEPAGE";
			interrupts = <3 0 0>;
			interrupt-parent = <&gpa0>;
			linux,code = <172>;
			gpios = <&gpa0 3 0xf>;
			gpio-key,wakeup = <1>;
		};
	};

	pinctrl@14CC0000 {
		/* touchkey i2c*/
		hs_i2c9_bus: hs-i2c9-bus {
			samsung,pins = "gpb0-3", "gpb0-2";
			samsung,pin-function = <4>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		cfg_i2c: cfg-i2c {
			samsung,pins = "gpb0-3", "gpb0-2";
			samsung,pin-function = <4>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		cfg_gpio: cfg_gpio {
			samsung,pins = "gpb0-3", "gpb0-2";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@10580000 {
		/* touchkey interrupt*/
		cfg_irq: cfg-irq {
			samsung,pins = "gpa3-2";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@14CE0000 {
		hs_i2c5_bus: hs-i2c5-bus {
			samsung,pins = "gpj1-1", "gpj1-0";
			samsung,pin-function = <4>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <1>;
		};
	};

	pinctrl@10580000 {
		mfd_irq: mfd-irq {
			samsung,pins = "gpa1-5";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		fuel_irq: fuel-irq {
			samsung,pins = "gpa1-4";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		muic_reset_irq: muic-reset-irq {
			samsung,pins = "gpa3-4";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		if_con_sense_irq: if_con_sense-irq {
			samsung,pins = "gpa3-5";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	hsi2c@14D90000 {
		max77888@66 {
			compatible = "maxim,max77888";
			reg = <0x66>;
			pinctrl-names = "default";
			pinctrl-0 = <&mfd_irq &if_con_sense_irq &fuel_irq &vibetonz_pwm &muic_reset_irq>;
			max77888,irq-gpio = <&gpa1 5 1>;
			max77888,muic-reset-int = <&gpa3 4 1>;
			max77888,irqf-trigger = <0x00002008>; /* IRQF_ONESHOT | IRQF_TRIGGER_LOW */
			max77888,wakeup;

			regulators {
				safeout1: ESAFEOUT1 {
					regulator-compatible = "safeout1";
					regulator-name = "safeout1_range";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <4900000>;
					regulator-boot-on;
				};

				safeout2: ESAFEOUT2 {
					regulator-compatible = "safeout2";
					regulator-name = "safeout2_range";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <4900000>;
				};
			};
		};
	};

	haptic {
		status = "okay";
		haptic,max_timeout = <10000>;
		haptic,duty = <32775>;
		haptic,period = <38240>;
		haptic,reg2 = <0x82>;
		haptic,pwm_id = <0>;
		haptic,regulator_name = "VCC_3.0V_MOTOR_AP";
	};

	rgb {
		rgb-name = "led_w", "led_r", "led_g", "led_b";
	};

	hall {
		status = "okay";
		compatible = "hall";
		pinctrl-names = "default";
		pinctrl-0 = <&hall_irq>;
		hall,gpio_flip_cover = <&gpa0 1 0>;
	};

	usb_notifier {
		compatible = "samsung,usb-notifier";
	};

	muic {
		status = "disabled";
		muic,rustproof_inbattery;
		muic,support-list = "MHL","OTG","Charging Cable","Jig USB On","Jig UART Off",
				    "Jig UART Off + VB","Jig UART On",
				    "TA","USB","CDP","Undefined Charging",
				    "Unofficial ID","Unofficial ID + TA","Unofficial ID + ANY TA",
				    "Unofficial ID + USB","Unofficial ID + CDP",
				    "Deskdock","Deskdock + VB",
				    "USB LANHUB","Universal Multimedia dock";
		muic,afcmode-tx = /bits/ 8 <0x46>;
	};

/*SENSOR*/
	pinctrl@10580000 {
		bma_irq: bma_irq {
			samsung,pins = "gpa0-0", "gpa0-5";
			samsung,pin-function = <0xf>;
			samsung,pin-con-pdn = <0>;
			samsung,pin-pud-pdn = <0>;
		};
		gyro_drdy: gyro_drdy {
			samsung,pins = "gpa0-6";
			samsung,pin-function = <0>;
			samsung,pin-con-pdn = <0>;
			samsung,pin-pud-pdn = <3>;
		};
	};
	pinctrl@14CC0000 {
		magnetic_rst: magnetic_rst {
			samsung,pins = "gpd4-3";
			samsung,pin-function = <1>;
			samsung,pin-con-pdn = <0>;
			samsung,pin-pud-pdn = <3>;
		};
	};
	hsi2c@14EC0000 {
		/*samsung,hs-mode;*/
		/*clock-frequency = <400000>;*/
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-scl-delay = <100>;
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&hs_i2c4_bus &bma_irq &gyro_drdy &magnetic_rst>;
		accelerometer@18 {
			compatible = "accelerometer,bma255-i2c";
			reg = <0x18>;
			interrupt-parent = <&gpa0>;
			interrupts = <5 0 0>;
			accelerometer,sda-gpio = <&gpj0 0 0>;
			accelerometer,scl-gpio = <&gpj0 1 0>;
			accelerometer,int1_gpio= <&gpa0 5 0>;
			accelerometer,chip_pos = <7>;
		};
		gyro@68 {
			compatible = "gyro,bmg160-i2c";
			reg = <0x68>;
			interrupt-parent = <&gpa0>;
			interrupts = <0 0 0>;
			gyro,sda-gpio = <&gpj0 0 0>;
			gyro,scl-gpio = <&gpj0 1 0>;
			gyro,int2_gpio=<&gpa0 0 0>;
			gyro,drdy-gpio= <&gpa0 6 0>;
			gyro,chip_pos = <7>;
		};
		magnetic_sensor@0c {
			compatible = "magnetic_sensor,ak0911c";
			reg = <0x0c>;
			magnetic_sensor,sda-gpio = <&gpj0 0 0>;
			magnetic_sensor,scl-gpio = <&gpj0 1 0>;
			magnetic_sensor,m_rst_n = <&gpd4 3 0>;
			magnetic_sensor,chip_pos = <4>;
		};
		light_sensor@10 {
			compatible = "light_sensor,cm3323";
			reg = <0x10>;
			light_sensor,sda-gpio = <&gpj0 0 0>;
			light_sensor,scl-gpio = <&gpj0 1 0>;
		};
	};

	hsi2c@14DA0000 {
		status = "okay";
		pinctrl-names = "default","on_i2c","off_i2c";
		pinctrl-0 = <&hs_i2c9_bus>;
		pinctrl-1 = <&cfg_i2c>;
		pinctrl-2 = <&cfg_gpio>;
		touchkey@60 {
			pinctrl-names = "default";
			pinctrl-0 = <&cfg_irq>;
			compatible = "coreriver,tc300-keypad";
			reg = <0x60>;
			interrupts = <2 0 0>;
			interrupt-parent = <&gpa3>;
			coreriver,sda-gpio = <&gpb0 2 0>;
			coreriver,scl-gpio = <&gpb0 3 0>;
			coreriver,irq-gpio = <&gpa3 2 1>;
			coreriver,regulator_ic = "vtouch_1.8v";
			coreriver,regulator_led = "vtouch_3.3v";
			coreriver,fw_name = "coreriver/tc300k_chagall.fw";
			coreriver,fw_version = <0x1B>;
			coreriver,sensing_ch_num = <0x6>;
		};
	};

	hsi2c@14E40000 {
		status = "okay";
		samsung,hs-mode;
		clock-frequency = <2500000>;

		s2mps13_pmic@66 {
			compatible = "samsung,s2mps13-pmic";
			reg = <0x66>;
			interrupts = <7 0 0>;
			interrupt-parent = <&gpa0>;
			pinctrl-names = "default";
			pinctrl-0 = <&pmic_irq &pm_wrsti>;
			gpios = <&gpa3 7 0>, <&gpf0 1 0x0>, <&gpg1 1 0x1>;
			smpl_warn_vth = <0x40>; /* 2.5V */
			smpl_warn_en = <1>; /* 1 : enable , 0 : disable*/
			g3d_en = <0>; /* 1 : enable , 0 : disable */
			dvs_en = <1>; /* 1 : enable , 0 : disable */
			/* RTC: wtsr/smpl */
			wtsr_en = <1>; /* enable */
			smpl_en = <1>; /* enable */
			wtsr_timer_val = <3>;	/* 1000ms */
			smpl_timer_val = <4>;	/* 500ms */
			check_jigon = <1>; /* check jigon */
			/* RTC: If it's first boot, reset rtc to 1/1/2014 12:00:00(Wed) */
			init_time,sec = <0>;
			init_time,min = <0>;
			init_time,hour = <12>;
			init_time,mday = <1>;
			init_time,mon = <0>;
			init_time,year = <114>;
			init_time,wday = <3>;
		};
	};

	pinctrl@10580000 {
		hall_irq: hall-irq {
			samsung,pins = "gpa0-1";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	ion {
		compatible = "samsung,exynos5430-ion";

		contig-names = "common",
			       "mfc_sh",
			       "g2d_wfd",
			       "video",
			       "sectbl",
			       "mfc_fw",
			       "mfc_nfw",
			       "secdma";

		contig = <0 0x600000 0x0>,
		         <2 0x100000 0x0>,
		         <10 0x800000 0x0>,
		         <11 0xAC00000 0x0>,
		         <9 0x100000 0x0>,
		         <8 0x100000 0x0>,
		         <12 0x100000 0x0>,
		         <13 0x20000 0x0>;

		contig-isolate_on_boot = <0>, <2>, <8>,
					  <9>, <10>, <11>, <12>, <13>;

		ion_noncontig_heap {
			id-type = <0 0>;
		};

		exynos_contig_heap {
			id-type = <4 6>;
		};
	};

	pinctrl@10580000 {
		disp_id: disp_id {
			samsung,pins = "gpf2-1";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
		};
	};

	pinctrl@14CC0000 {
		disp_errfg: disp_errfg {
			samsung,pins = "gpb0-4";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <1>;
		};
	};

	/* disp_driver */
	decon_fb: decon_fb {
		oled-pcd-gpio = <&gpf1 6 0x0>;
		oled-errfg-gpio = <&gpb0 4 0xf>;
		gpio-tcon-rdy = <&gpf1 6 0x0>;
		lcd_info = <&s6tnmr7>;
		decon_board = <&decon_board_list>;

		pinctrl-names = "default", "turnon_tes", "turnoff_tes";
		pinctrl-0 = <&disp_id &disp_errfg>;
		pinctrl-1 = <&disp_id &disp_errfg &disp_teson>;
		pinctrl-2 = <&disp_id &disp_tesoff>;

		decon: decon_ctrl {
			samsung,max_bpp = <32>;
			samsung,default_bpp = <24>;

			fb_driver_data {
				fb_win_variant_0 {
					has_osd_c = <1>;
					has_osd_d = <1>;
					osd_size_off = <0x08>;
					palette_size = <256>;
					VALID_BPP_1248 = <1>;
					VALID_BPP_13 = <1>;
					VALID_BPP_15 = <1>;
					VALID_BPP_16 = <1>;
					VALID_BPP_18 = <1>;
					VALID_BPP_19 = <1>;
					VALID_BPP_24 = <1>;
					VALID_BPP_25 = <1>;
					VALID_BPP_32 = <1>;
				};
				fb_win_variant_1 {
					has_osd_c = <1>;
					has_osd_d = <1>;
					has_osd_alpha = <1>;
					osd_size_off = <0x0c>;
					palette_size = <256>;
					VALID_BPP_1248 = <1>;
					VALID_BPP_13 = <1>;
					VALID_BPP_15 = <1>;
					VALID_BPP_16 = <1>;
					VALID_BPP_18 = <1>;
					VALID_BPP_19 = <1>;
					VALID_BPP_24 = <1>;
					VALID_BPP_25 = <1>;
					VALID_BPP_32 = <1>;
				};
				fb_win_variant_2 {
					has_osd_c = <1>;
					has_osd_d = <1>;
					has_osd_alpha = <1>;
					osd_size_off = <0x0c>;
					palette_size = <256>;
					VALID_BPP_1248 = <1>;
					VALID_BPP_13 = <1>;
					VALID_BPP_15 = <1>;
					VALID_BPP_16 = <1>;
					VALID_BPP_18 = <1>;
					VALID_BPP_19 = <1>;
					VALID_BPP_24 = <1>;
					VALID_BPP_25 = <1>;
					VALID_BPP_32 = <1>;
				};
				fb_win_variant_3 {
					has_osd_c = <1>;
					has_osd_alpha = <1>;
